{
  "name": "popular-quotes",
  "version": "1.0.8",
  "description": "A package for popular quotes from various media.",
  "main": "index.js",
  "scripts": {
    "build":"parcel build index.js --out-file dist/index.js --target node",
    "test": "npm run test-with-coverage",
    "test-with-coverage": "jest -u --coverage && cat ./coverage/lcov.info | codacy-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Snipey/popular-quotes.git"
  },
  "author": "Stephen Freerking",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Snipey/popular-quotes/issues"
  },
  "homepage": "https://github.com/Snipey/popular-quotes#readme",
  "devDependencies": {
    "codacy-coverage": "^3.4.0",
    "jest": "^24.9.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
